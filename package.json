  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "knex": "^3.1.0",
    "module-alias": "^2.2.3",
    "nodemon": "^3.1.0",
    "pg": "^8.11.5",
    "rimraf": "^5.0.5",
    "socket.io": "^4.7.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/pg": "^8.11.5"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
